<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.oemden.claudesecrets</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/claudesecrets</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
